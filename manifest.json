{
  "name": "SciRate Assistant",
  "version": "1.0",
  "description": "Remove Sen",
  "permissions": ["activeTab", "declarativeContent", "storage", "tabs", 
    "http://*/",
    "https://*/"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/sa16.png",
      "32": "images/sa32.png",
      "48": "images/sa48.png",
      "128": "images/sa128.png"
    }
  },
  "icons": {
    "16": "images/sa16.png",
    "32": "images/sa32.png",
    "48": "images/sa48.png",
    "128": "images/sa128.png"
  },
  "manifest_version": 2
}
